{
	"AuthMaxAge": 60,

	"Console": false,
	"Refresh": false,
	"LogDir": "/var/log/cmdbd",

	"ConfigFile": {
		"SystemLog": "logger/system.json",
		"AccessLog": "logger/system.json",
		"ErrorLog": "logger/system.json",
		"DataStore": "store/mysql.json",
		"Queries": "model/queries.json",
		"MetaUsb": "cache/metausb.json",
		"Syslog": "server/syslog.json",
		"Router": "server/router.json",
		"Server": "server/httpd.json"
	},

	"CryptoFile": {
		"PriKey": "crypto/cmdbd.rsa",
		"PubKey": "crypto/cmdbd.pub"
	},

	"SerialFormat": {
		"*usb.Magtek": "24HF%04XMT",
		"*usb.IDTech": "24HF%04XIT",
		"*usbci.Magtek": "24HF%04XMT",
		"Default": "24HF%06X"
	}
}
